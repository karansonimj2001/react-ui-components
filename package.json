{
  "name": "react-ui-components",
  "version": "1.0.0",
  "description": "Production-ready React UI components with TypeScript, TailwindCSS, and Storybook",
  "homepage": "https://karansonimj2001.github.io/react-ui-components",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
